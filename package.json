{
  "name": "mwapp",
  "version": "0.1.2",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "husky": "^0.14.3",
    "jquery": "^3.3.1",
    "lint-staged": "^7.2.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "popper.js": "^1.14.4",
    "prettier": "^1.14.0",
    "react": "^16.4.2",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-snapshot": "^1.3.0",
    "reactstrap": "^6.3.1",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "postmerge-bump-patch": "npm version patch",
    "precommit-lint": "lint-staged",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "precommit": "npm run precommit-lint",
    "postmerge": "npm run postmerge-bump-patch",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p start-js watch-css",
    "build-js": "react-app-rewired build",
    "build-static": "react-snapshot",
    "build": "npm-run-all build-css build-js build-static",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
